{
    "name": "oguzhankrcb/datamigrator",
    "description": "A simple package for data migration",
    "keywords": [
        "oguzhankrcb",
        "laravel",
        "datamigrator"
    ],
    "homepage": "https://github.com/oguzhankrcb/datamigrator",
    "license": "MIT",
    "authors": [
        {
            "name": "OÄŸuzhan KARACABAY",
            "email": "oguzhankrcb@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "9.*|^10.0",
        "illuminate/support": "9.*|^10.0",
        "illuminate/database": "9.*|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "phpunit/phpunit": "9.*|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Oguzhankrcb\\DataMigrator\\": "src",
            "Oguzhankrcb\\DataMigrator\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oguzhankrcb\\DataMigrator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oguzhankrcb\\DataMigrator\\DataMigratorServiceProvider"
            ],
            "aliases": {
                "DataMigrator": "Oguzhankrcb\\DataMigrator\\Facades\\DataMigrator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
